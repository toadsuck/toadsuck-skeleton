{
	"name": "toadsuck/skeleton",
	"description": "Skeleton Application for Toadsuck",
	"homepage": "https://github.com/toadsuck/toadsuck-skeleton",
	"license": "MIT",
	"authors": [
		{
			"name": "Josh Moody",
			"email": "jgmoody@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Toadsuck\\Skeleton\\Tests\\": "tests/",
			"Toadsuck\\Skeleton\\Controllers\\": "src/controllers/",
			"Toadsuck\\Skeleton\\Entities\\": "src/entities/",
			"Toadsuck\\Skeleton\\Managers\\": "src/managers/",
			"Toadsuck\\Skeleton\\Models\\": "src/models/",
			"Toadsuck\\Skeleton\\Utils\\": "src/utils/",
			"Toadsuck\\Skeleton\\Validators\\": "src/validators/"
		}
	},
	"require": {
		"php": ">= 5.4",
		"toadsuck/core": "dev-develop-2",
		"werx/validation": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"squizlabs/php_codesniffer": "1.*"
	},
	"minimum-stability": "dev"
}
